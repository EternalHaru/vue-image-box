{
  "name": "vue-image-box",
  "version": "1.0.1",
  "description": "Lightweight and simple image gallery component for Vue.js",
  "author": "Adam Romig",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/aromig/vue-image-box"
  },
  "main": "dist/vue-image-box.umd.js",
  "module": "dist/vue-image-box.esm.js",
  "unpkg": "dist/vue-image-box.min.js",
  "browser": {
    "./sfc": "src/vue-image-box.vue"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "cross-env": "^5.2.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.11.0",
    "rollup": "^1.4.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.8",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^4.0.0"
  }
}
